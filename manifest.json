{
  "manifest_version": 2,
  "name": "Hugo Zdoc theme",
  "short_name": "Hugo Zdoc theme",
  "version": "1.0.0",
  "default_locale": "en",
  "description": "Hugo simple documentation theme",
  "start_url": "https://danacoding.github.io/",
  "icons": [
    {
      "src": "favicon/favicon.png",
      "sizes": "36x36",
      "type": "image/png",
      "density": "0.75"
    },
    {
      "src": "favicon/favicon.png",
      "sizes": "48x48",
      "type": "image/png",
      "density": "1.0"
    },
    {
      "src": "favicon/favicon.png",
      "sizes": "72x72",
      "type": "image/png",
      "density": "1.5"
    },
    {
      "src": "favicon/favicon.png",
      "sizes": "96x96",
      "type": "image/png",
      "density": "2.0"
    },
    {
      "src": "favicon/favicon.png",
      "sizes": "144x144",
      "type": "image/png",
      "density": "3.0"
    },
    {
      "src": "favicon/favicon.png",
      "sizes": "192x192",
      "type": "image/png",
      "density": "4.0"
    },
    {
      "src": "favicon/favicon.png",
      "sizes": "512x512",
      "type": "image/png",
      "density": "4.0"
    }
  ],
  "theme_color": "#0071bc",
  "background_color": "#fff",
  "display": "standalone"
}
